extends layout

block content
  h1= title
  - let image_names = ["machupichu.jpg", "k2.jpg", "mount colorado.jpeg", "mount everest.jpg", "Mountfuji.jpg"]
  
  - for (let i = 0; i < 3; i++)
    - let randomIndex = Math.floor(Math.random() * 5)
    - let imageName = image_names[randomIndex]
    img(src=`/images/${imageName}`, alt=imageName, height="100", width="100")
